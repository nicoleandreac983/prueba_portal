<h1>Nuevas Ofertas Laborales</h1>

<%= form_with(model: @job_offer, url: job_offers_path) do |form| %>
    <div>
        <%= form.label :title %>
        <%= form.text_field :title %>
    </div>

    <div>
        <%= form.label :description %>
        <%= form.text_area :description %>
    </div>

    <div>
        <%= form.submit "Crear Oferta Laboral" %>
    </div>
<% end %>